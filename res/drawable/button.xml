<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<!--点击背景-->
    <item android:state_pressed="true">
        <bitmap android:src="@drawable/popbg_pressed" />
    </item>
    <!--默认背景-->
    <item>
		<shape>
        	<solid android:color="#00ffffff" />
